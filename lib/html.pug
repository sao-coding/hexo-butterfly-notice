#noticeList.container
    i.notice-logo.fas.fa-bullhorn.faa-pulse.animated

    .swiper-container
        #notice-item.swiper-wrapper
            if notice
                each i in notice
                    .li-style.swiper-slide=i.msg
    i.notice-enter.fas.fa-arrow-circle-right(title="查看全文")

